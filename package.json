{
  "name": "sunrise-scenarios",
  "version": "0.1.0",
  "description": "Test suite for the CTP Sunrise application",
  "scripts": {
    "test": "npm run lint && NODE_ENV=test mocha --compilers js:babel-register --recursive test/*.spec.js",
    "test:watch": "npm run test -- --watch",
    "test:features": "./scripts/features_run",
    "test:features:ci": "NODE_TLS_REJECT_UNAUTHORIZED=0 NODE_ENV=ci cucumber.js",
    "test:features:local:chrome": "NODE_BROWSER=chrome ./scripts/features_run",
    "test:features:local:ci": "NODE_TLS_REJECT_UNAUTHORIZED=0 NODE_ENV=ci ./scripts/features_run_docker"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/commercetools-sunrise-scenarios"
  },
  "bugs": {
    "url": "https://github.com/commercetools/commercetools-sunrise-scenarios/issues",
    "email": "support@sphere.io"
  },
  "dependencies": {},
  "devDependencies": {
    "colors": "^1.1.2",
    "cucumber": "^0.10.1",
    "cz-conventional-changelog": "1.1.5",
    "eslint": "~2.7.0",
    "eslint-plugin-mocha": "^4.0.0",
    "mocha": "^2.4.5",
    "progress": "^1.1.8",
    "selenium-standalone": "^5.1.0",
    "validate-commit-msg": "^2.5.0",
    "webdriverio": "^4.2.3"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=2"
  },
